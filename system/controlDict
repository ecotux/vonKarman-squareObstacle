FoamFile
{
	version		2.0;
	format		ascii;
	class		dictionary;
	object		controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application		pimpleFoam;
startFrom		startTime;
startTime		0;
stopAt			endTime;
endTime			1.5;
deltaT			0.00005;
writeControl		timeStep;
writeInterval		50;
purgeWrite		0;
writeFormat		ascii;
writePrecision		20;
writeCompression	uncompressed;
timeFormat		general;
timePrecision		6;
runTimeModifiable	yes;
adjustTimeStep		yes;
maxCo			1;

